{
  "name": "artist-next-site",
  "private": true,
  "version": "1.0.12",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "if [ -f public/_redirects ]; then mkdir -p out && cp public/_redirects out/_redirects; fi",
    "deploy": "npm run ingest && npm run sitemap && next build",
    "ingest:images": "node scripts/ingest-images.mjs",
    "gen:sitemap": "node scripts/generate-sitemap.mjs",
    "start": "next start",
    "ingest": "node scripts/ingest-images.mjs",
    "sitemap": "node scripts/generate-sitemap.mjs"
  },
  "dependencies": {
    "next": "14.2.10",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "fast-glob": "^3.3.3",
    "postcss": "^8.4.38",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.7"
  }
}
